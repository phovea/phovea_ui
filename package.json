{
  "name": "phovea_ui",
  "description": "Wrapper for integration of Bootstrap and FontAwesome along with a common header for Phovea apps",
  "version": "2.1.4-SNAPSHOT",
  "author": {
    "name": "The Caleydo Team",
    "email": "contact@caleydo.org",
    "url": "https://caleydo.org"
  },
  "license": "BSD-3-Clause",
  "homepage": "https://phovea.caleydo.org",
  "bugs": {
    "url": "https://github.com/phovea/phovea_ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phovea/phovea_ui.git"
  },
  "main": "build/phovea_ui.js",
  "files": [
    "src",
    "index.js",
    "phovea.js",
    "phovea_registry.js",
    "build"
  ],
  "engines": {
    "npm": ">= 6.12",
    "node": ">= 12.13",
    "iojs": ">= 3"
  },
  "scripts": {
    "compile": "tsc",
    "lint": "tslint -c tslint.json -p . 'src/**/*.ts?(x)' 'tests/**/*.ts?(x)'",
    "docs": "typedoc --options typedoc.json src/**.ts",
    "prebuild": "node -e \"process.exit(process.env.PHOVEA_SKIP_TESTS === undefined?1:0)\" || npm run test",
    "pretest": "npm run compile",
    "test": "test ! -d tests || karma start",
    "posttest": "npm run lint",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "build:dev": "webpack --mode=development",
    "build": "webpack --mode=production --verbose --display=verbose",
    "start": "webpack-dev-server --mode=development --inline",
    "start:hot": "webpack-dev-server --mode=development --inline --hot",
    "watch": "webpack --mode=development --watch",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "predist": "npm run build && npm run docs",
    "dist": "mkdirp dist && cd build && tar cvzf ../dist/phovea_ui.tar.gz *"
  },
  "dependencies": {
    "font-awesome": "~4.7.0",
    "@types/jquery": "~3.3.6",
    "jquery": "~3.3.1",
    "@types/bootstrap": "3.3.32",
    "bootstrap-sass": "3.3.7",
    "phovea_core": "github:phovea/phovea_core#stoiber/webpack4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.47",
    "awesome-typescript-loader": "^5.2.1",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.3.2",
    "extract-loader": "^3.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-loader": "^0.5.5",
    "ifdef-loader": "^2.1.4",
    "imports-loader": "^0.8.0",
    "jasmine": "2.5.3",
    "json-loader": "^0.5.7",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-firefox-launcher": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "2.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "mkdirp": "0.5.1",
    "node-sass": "^4.12.0",
    "null-loader": "^3.0.0",
    "raw-loader": "^4.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.2",
    "thread-loader": "^2.1.3",
    "ts-loader": "^6.2.1",
    "tslint": "5.9.1",
    "typedoc": "0.11.1",
    "typescript": "2.8.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.9.0",
    "tslib": "1.9.0"
  }
}
